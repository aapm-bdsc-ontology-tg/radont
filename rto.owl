        <obo:IAO_0000119>NCI website</obo:IAO_0000119>
        <rdfs:label>pathological stage</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/rto-draft#patient_positioner:radont_nnnn -->

    <owl:Class rdf:about="http://purl.org/rto-draft#patient_positioner:radont_nnnn">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0000968"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000050"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/rto-draft#RADONT_0000060"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>A device that is part of a radiation therapy device that is used to support and position  the patient with respect to the radiation field used to treat the patient.</obo:IAO_0000115>
        <rdfs:isDefinedBy>Mark Phillips</rdfs:isDefinedBy>
        <rdfs:label>patient positioner</rdfs:label>
    </owl:Class>
    


