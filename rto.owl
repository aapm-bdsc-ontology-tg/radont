        <rdfs:subClassOf rdf:resource="http://purl.org/rto-draft#RADONT_0000060"/>
        <obo:IAO_0000115>A radiation therapy device that generates radiation from some part of the device that is inserted into that part of a person&apos;s body at which the intended target tissue is located.</obo:IAO_0000115>
        <rdfs:isDefinedBy>Mark Phillips</rdfs:isDefinedBy>
        <rdfs:label>brachytherapy device</rdfs:label>
    </owl:Class>
    


