    <owl:Class rdf:about="http://purl.org/rto-draft#cyclotron:rto_nnnn">
        <rdfs:subClassOf rdf:resource="http://purl.org/rto-draft#external_beam_device"/>
        <obo:IAO_0000115>An external beam device that accelerates charged particles outwards from the center of a flat cylindrical vacuum chamber along a spiral path.</obo:IAO_0000115>
        <obo:IAO_0000119>https://en.wikipedia.org/wiki/Cyclotron</obo:IAO_0000119>
        <rdfs:label>cyclotron</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/rto-draft#data_item:IAO_0000027 -->

    <owl:Class rdf:about="http://purl.org/rto-draft#data_item:IAO_0000027">
