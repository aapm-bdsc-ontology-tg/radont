        <rdfs:label>bremmstrahlung</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/rto-draft#cancer_stage:rto_nnnn -->

    <owl:Class rdf:about="http://purl.org/rto-draft#cancer_stage:rto_nnnn">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000123"/>
