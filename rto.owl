        <rdfs:comment>Reviewed 10/26/22</rdfs:comment>
        <rdfs:isDefinedBy>Mark Phillips</rdfs:isDefinedBy>
        <rdfs:label>multileaf collimator</rdfs:label>
    </owl:Class>
    


